{
	"name": "Inspector JSON",
	"version": "0.0.1",
	"manifest_version": 2,
	"description": "A simple way to view JSON",
	"page_action": {
		"default_icon": "images/icons/inspector_json_20.png",
		"default_title": "Inspector JSON"
	},
	"permissions": ["tabs", "\u003Call_urls\u003E"],
	"background_page": "background.htm",
	"content_scripts": [
		{
			"all_frames": true,
			"css": ["stylesheets/inspector_json.css"],
			"js": [
				"scripts/vendor/jquery/1.7.2/jquery.js",
				"scripts/vendor/underscore/1.3.3/underscore.js",
				"scripts/vendor/underscore/mixins.js",
				"scripts/inspector_json.js",
				"scripts/extension.js"],
			"matches": ["\u003Call_urls\u003E"]
		}
	]
}